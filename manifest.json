{
  "content_scripts": [
    {
      "matches": ["https://coefont.cloud/editor/*"],
      "js": ["content-script.js"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "description": "When downloading a single wav file, you will also download a txt file.",
  "icons": {
    "128": "icon128.png"
  },
  "manifest_version": 3,
  "name": "CoeFont Studio Auto Text Downloader",
  "version": "1.8",
  "permissions": ["downloads"]
}